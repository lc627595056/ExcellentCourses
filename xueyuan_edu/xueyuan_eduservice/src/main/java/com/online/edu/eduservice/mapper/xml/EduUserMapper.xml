<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.online.edu.eduservice.mapper.EduUserMapper">

    <select id="selectUser" resultType="com.online.edu.eduservice.entity.EduUser">
        select username,password from edu_user where username=#{username} and password=#{password}
    </select>

</mapper>
